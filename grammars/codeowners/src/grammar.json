{
  "name": "codeowners",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "section"
          },
          {
            "type": "SYMBOL",
            "name": "entry"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_text"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "comment_text": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "section_name"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "section_approvers"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "section_name": {
      "type": "PATTERN",
      "value": "[^\\]]+"
    },
    "section_approvers": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\s+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "owner"
          }
        }
      ]
    },
    "entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "pattern",
          "content": {
            "type": "SYMBOL",
            "name": "pattern"
          }
        },
        {
          "type": "FIELD",
          "name": "owners",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "owner"
            }
          }
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "pattern": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "wildcard_pattern"
        },
        {
          "type": "SYMBOL",
          "name": "directory_pattern"
        },
        {
          "type": "SYMBOL",
          "name": "file_pattern"
        },
        {
          "type": "SYMBOL",
          "name": "glob_pattern"
        }
      ]
    },
    "wildcard_pattern": {
      "type": "PATTERN",
      "value": "\\*+"
    },
    "directory_pattern": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "/"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\\s#@]+"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "/"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    },
    "file_pattern": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "/"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "[^\\s#@*]+"
          }
        ]
      }
    },
    "glob_pattern": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "/"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\\s#@*]+"
                },
                {
                  "type": "STRING",
                  "value": "*"
                },
                {
                  "type": "STRING",
                  "value": "**"
                }
              ]
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "/"
                },
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "[^\\s#@*]+"
                      },
                      {
                        "type": "STRING",
                        "value": "*"
                      },
                      {
                        "type": "STRING",
                        "value": "**"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    },
    "owner": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "username"
        },
        {
          "type": "SYMBOL",
          "name": "team"
        },
        {
          "type": "SYMBOL",
          "name": "email"
        }
      ]
    },
    "username": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "team": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "FIELD",
          "name": "org",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "email": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9._%+-]+"
          },
          {
            "type": "STRING",
            "value": "@"
          },
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9.-]+"
          },
          {
            "type": "STRING",
            "value": "."
          },
          {
            "type": "PATTERN",
            "value": "[a-zA-Z]{2,}"
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
